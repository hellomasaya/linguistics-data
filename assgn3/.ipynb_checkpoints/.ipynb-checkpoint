    "def find_map_with_data(ud_tags, ann_tags):\n",
    "    new_map = {}\n",
    "#     print(len(ud_tags), len(ann_tags))\n",
    "    for i,ud in enumerate(ud_tags):\n",
    "        new_map.setdefault(ud,[])\n",
    "        if(ann_tags[i] not in new_map[ud]):\n",
    "            new_map[ud].append(ann_tags[i])\n",
    "    return new_map"
   ]
